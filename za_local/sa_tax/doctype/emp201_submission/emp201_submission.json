        },
        {
            "fieldname": "submission_period_start_date",
            "label": "Submission Period Start Date",
            "fieldtype": "Date",
            "reqd": 1,
            "in_list_view": 1
        },
        {
            "fieldname": "submission_period_end_date",
            "label": "Submission Period End Date",
            "fieldtype": "Date",
            "reqd": 1,
            "in_list_view": 1
        },
        {
            "fieldname": "status",
            "label": "Status",
            "fieldtype": "Select",
            "options": "Draft\nSubmitted to SARS\nCancelled",
