            "options": "company:currency",
            "read_only": 1
        },
        {
            "fieldname": "uif_payable",
            "label": "UIF Payable",
            "fieldtype": "Currency",
            "options": "company:currency",
            "read_only": 1,
            "bold": 1
        },
        {
            "fieldname": "sdl_payable",
            "label": "SDL Payable",
            "fieldtype": "Currency",
            "options": "company:currency",
            "read_only": 1,
            "bold": 1
        }
    ]
