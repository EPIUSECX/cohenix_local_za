    "permissions": [
        {
            "role": "Payroll Manager",
            "read": 1,
            "write": 1,
            "create": 1,
            "delete": 1,
            "submit": 1,
            "cancel": 1,
            "report": 1
        },
        {
            "role": "Accounts User",
            "read": 1
        }
    ],
    "autoname": "EMP201-.#####",
    "naming_rule": "Expression (autoname)",
